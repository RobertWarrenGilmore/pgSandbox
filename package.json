{
  "name": "pgSandbox",
  "version": "0.1.0",
  "description": "a first attempt at a database wrapper app over postgres",
  "scripts": {
    "start": "sudo $(which node) node_modules/.bin/forever start index.js",
    "stop": "sudo $(which node) node_modules/.bin/forever stop index.js",
    "dbSetup": "server/database/dbSetup.sh",
    "cert": "ssl/generateSelfSignedCertificate.sh",
    "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "lint": "node_modules/.bin/eslint --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertWarrenGilmore/pgSandbox.git"
  },
  "keywords": [
    "postgres",
    "express",
    "nodejs",
    "node"
  ],
  "author": "Robert Warren Gilmore (Robert.Warren.Gilmore@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobertWarrenGilmore/pgSandbox/issues"
  },
  "devDependencies": {
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^2.6.4",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "sinon": "^1.17.1"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "bcrypt": "^0.8.5",
    "bluebird": "^3.0.1",
    "body-parser": "^1.14.1",
    "browserify": "^12.0.0",
    "checkit": "^0.5.1",
    "classnames": "^2.2.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "forever": "^0.15.1",
    "history": "^1.12.6",
    "knex": "^0.8.6",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.1",
    "nodemailer": "^1.8.0",
    "nodemailer-sparkpost-transport": "^0.1.0",
    "pg": "^4.4.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3",
    "reactify": "^1.1.1",
    "request": "^2.65.0",
    "uglifyify": "^3.0.1"
  },
  "engines": {
    "node": "5.0.0"
  }
}
